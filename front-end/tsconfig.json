{
    "compilerOptions": {
        "module": "es6",
        "target": "es5",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "outDir": "./dist",
        "rootDir": "./src",
        "baseUrl": ".",
        "sourceMap": true,
        "downlevelIteration": true,
        "noImplicitAny": false,
        "paths": {
            "*": [
                "node_modules/*"
            ]
        },
    },
    "include": [
        "src/**/*"
    ],
}